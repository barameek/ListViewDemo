<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.barameek.listviewdemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PageConfig"
            android:label="@string/title_activity_page_config" >
        </activity>
        <activity
            android:name=".PageData"
            android:label="@string/title_activity_page_data" >
        </activity>
        <activity
            android:name=".PageProfile"
            android:label="@string/title_activity_page_profile" >
        </activity>
    </application>

</manifest>
